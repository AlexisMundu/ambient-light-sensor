# ##############################################################################
# Constraints for Ambient Light Sensor project
# ##############################################################################

### Main FPGA Clock
set_io i_Clk 15

### LED Pins:
set_io o_LED_1 56
set_io o_LED_2 57
set_io o_LED_3 59
set_io o_LED_4 60

## Push-Button Switches
set_io i_Switch_1 53

## UART Outputs
set_io o_UART_TX 74

## PMOD Signals
set_io o_SPI_CS_n 65
set_io o_SPI_MOSI 64
set_io i_SPI_MISO 63
set_io o_SPI_Clk 62
